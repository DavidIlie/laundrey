{
   "cli": {
      "version": ">= 3.3.0"
   },
   "build": {
      "development": {
         "developmentClient": true,
         "distribution": "internal",
         "ios": {
            "resourceClass": "m-medium"
         }
      },
      "preview": {
         "distribution": "internal",
         "ios": {
            "simulator": true,
            "resourceClass": "m-medium"
         }
      },
      "production": {
         "ios": {
            "resourceClass": "m-medium"
         }
      }
   },
   "submit": {
      "production": {}
   },
   "expo": {
      "plugins": [
         [
            "expo-camera",
            {
               "cameraPermission": "Allow Laundrey to access your camera."
            }
         ]
      ]
   }
}
